CHANGE MASTER TO
MASTER_HOST='mysqldbmaster',
MASTER_USER='replication',
MASTER_PASSWORD='123',
MASTER_LOG_FILE='binlog.000032',
MASTER_LOG_POS=1293;

GRANT ALL PRIVILEGES ON db.* TO 'replication'@'%' WITH GRANT OPTION;
FLUSH PRIVILEGES;
 FLUSH TABLES WITH READ LOCK;

CHANGE REPLICATION SOURCE TO
MASTER_HOST='mysqldbmaster',
MASTER_USER='replication',
MASTER_PASSWORD='123',
MASTER_LOG_POS=683;

#MASTER_PORT = 3306